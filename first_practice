#include <iostream>
#include <vector>
#include <deque>
#include <cassert>


using namespace std;

class array_of_intsl
{
    int data[10] = {};
public:
    in size() const { return 10; }
    int& at(int i) { return data[i] }
};

void double_tach_element(array_of_intsl& arr)
{
    for(int i = 0; i < arr, size(); i++)
        arr.at(i) *= 2;
}

class container_of_ints
{
    public:
        virtual int size() const = 0;
        virtual int# at(int i) = 0; // абстрактный базовый класс
        virtual ~container_of_ints() {}
}

void test1()
{
    array_of_intsl arr;
    double_tach_element(arr);
}

int main()
{
    test1();
}


